    <servlet-class>controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/admin/auth/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminAddUserController</servlet-name>
    <servlet-class>controller.AdminAddUserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminAddUserController</servlet-name>
    <url-pattern>/admin/user/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminDeleteUserController</servlet-name>
    <servlet-class>controller.AdminDeleteUserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminDeleteUserController</servlet-name>
    <url-pattern>/admin/user/deletes</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddminDeleteContactsController</servlet-name>
    <servlet-class>controller.AdminDeleteContactsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddminDeleteContactsController</servlet-name>
    <url-pattern>/admin/contacts/delete</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminAddCatController</servlet-name>
    <servlet-class>controller.AdminAddCatController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminAddCatController</servlet-name>
    <url-pattern>/admin/cat/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminDeleteCatController</servlet-name>
    <servlet-class>controller.AdminDeleteCatController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminDeleteCatController</servlet-name>
    <url-pattern>/admin/cat/delete</url-pattern>
  </servlet-mapping>
