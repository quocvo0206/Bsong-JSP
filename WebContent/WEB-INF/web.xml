  <servlet>
    <servlet-name>AdminDeleteSongController</servlet-name>
    <servlet-class>controller.AdminDeleteSongController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminDeleteSongController</servlet-name>
    <url-pattern>/admin/song/delete</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>PublicIndexSongController</servlet-name>
    <servlet-class>controller.PublicIndexSongController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PublicIndexSongController</servlet-name>
    <url-pattern>/home/song</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>15</session-timeout>
  </session-config>
</web-app>